{
  "name": "@solido/atlante-js",
  "version": "0.4.4",
  "description": "JS Http Client",
  "license": "MIT",
  "directories": {
    "lib": "lib"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "coverage": "nyc npm test",
    "coverage-html": "nyc --reporter=html npm test",
    "lint": "eslint lib/",
    "lint-fix": "eslint --fix lib/",
    "test": "node ./tests.cjs"
  },
  "author": "Alessandro Chitolina <alekitto@gmail.com>",
  "dependencies": {
    "@jymfony/contracts": "^0.1.0-alpha.29",
    "@jymfony/exceptions": "^0.1.0-alpha.29",
    "@jymfony/util": "^0.1.0-alpha.29"
  },
  "devDependencies": {
    "@jymfony/autoloader": "^0.1.0-alpha.29",
    "@jymfony/compiler": "^0.2.10",
    "@jymfony/datetime": "^0.1.0-alpha.29",
    "@jymfony/debug": "^0.1.0-alpha.29",
    "@jymfony/decorators": "^0.1.0-alpha.29",
    "@jymfony/testing": "^0.1.0-alpha.29",
    "@jymfony/var-dumper": "^0.1.0-alpha.29",
    "@types/chai": "^4.2.11",
    "@types/node": "^12.19.6",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "axios": "^0.21",
    "chai": "^4.2.0",
    "eslint": "^7.14.0",
    "mocha": "^6.2.0",
    "node-localstorage": "^1.3.1",
    "nyc": "^15.1.0",
    "typescript": "^4.4.2",
    "whatwg-url": "^7.0.0"
  },
  "config": {
    "jymfony-autoload": {
      "namespaces": {
        "Solido.Atlante": "lib"
      }
    },
    "jymfony-autoload-dev": {
      "namespaces": {
        "Solido.Atlante.Stubs": "stubs/",
        "Solido.Atlante.Tests": "test/"
      }
    }
  },
  "nyc": {
    "per-file": true,
    "hookRequire": true,
    "hookRunInContext": true,
    "hookRunInThisContext": true,
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "**/tests.js",
      "**/namespace-stub.js",
      "**/fixtures/**",
      "**/test/**"
    ],
    "reporter": [
      "text-summary",
      "text",
      "clover"
    ],
    "all": true,
    "report-dir": "./coverage"
  }
}
